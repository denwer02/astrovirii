for console

import sys   
f = sys.argv[1] 
hostlist = []
i = 0 
hostlist = [] 
with open(f,'r') as a: 
     for line in a: 
        if line.find('/host') != -1: 
            line.strip() 
            number1 = line.find('"')
            number1 = number1+1
            number2 = line.find('"',number1)
            hostlist.append(line[number1:number2]) 
            i += 1 
hostlist.sort()
print(hostlist)
wod=set(hostlist)
with open('HostList.txt','w') as b:
    for l in wod:
        b.write(l)


for Jupiter
  
import sys   
i = 0 
hostlist = [] 
with open('sequence.gb','r') as a: 
     for line in a: 
        if line.find('/host') != -1: 
            line.strip() 
            number1 = line.find('"')
            number1 = number1+1
            number2 = line.find('"',number1)
            hostlist.append(line[number1:number2]) 
            i += 1 
hostlist.sort()
print(hostlist)
wod=set(hostlist)
with open('HostList.txt','w') as b:
    for l in wod:
        b.write(l)
